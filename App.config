<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1" />
  </startup>
  <appSettings>
    <add key="sqlTimeout" value="180"/>
    <add key="sqlBaglanti" value="Data Source=.;Initial Catalog=;User ID=;Password="/>
    <!--Sql bilgisi-->
    <add key="Debug" value="1" />
    <add key="mailLoginGonderen" value=""/>
    <!--Gönderecek mail-->
    <add key="mailSmtpGonderen" value=""/>
    <!--Gönderecek mail-->
    <add key="mailLoginGonderenAd" value=""/>
    <!--Maili gönderenin görünen adı-->
    <add key="mailSifre" value=""/>
