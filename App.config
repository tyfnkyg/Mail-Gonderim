    <add key="mailSmtpSsl" value="true"/>
    <add key="mailSmtpPort" value="587"/>
    <add key="mailSmtpHost" value="smtp.gmail.com"/>
    <add key="mailGonderCC" value=""/>
    <!--CC mailleri .-->
    <add key="mailDestekTalepleri" value=""/>
    <!--Günlük kaç tane açıldığı sorgusu  .-->
    <add key="mailGonderSorgu" value=""/>
    <!--Günlük toplam kaç tane talep açıldığı sorgusu  .-->
    <add key="mailGrafikSorgusu" value=""/>
    <!--Ay boyunca günlük açılan toplam destek sayısının sorgusu  .-->
    <add key="mailDestekBirOncekiGunKapatılanlar" value=""/>
    <!--Mersus içerisinde bir önceki gün kim ne kadar talep kapattı sorgusu-->
    <add key="mailDestekAyKapatılanlar" value=""/>
    <!--Bulunan ay içeirisinde kim toplam ne kadar destek kapattı-->
  </appSettings>
</configuration>